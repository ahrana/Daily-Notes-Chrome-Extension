{
  "manifest_version": 3,
  "name": "Daily Notes",
  "version": "1.0",
  "description": "Make daily notes with each line in different color.",
  "permissions": ["activeTab", "storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "logo.png",
      "48": "logo.png",
      "128": "logo.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}
